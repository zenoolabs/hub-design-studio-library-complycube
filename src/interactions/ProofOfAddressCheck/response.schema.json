{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Proof of Address Check Response",
  "description": "Response structure for ComplyCube proof of address verification",
  "type": "object",
  "properties": {
    "check_id": {
      "type": "string",
      "description": "Unique identifier for the verification check"
    },
    "outcome": {
      "type": "string",
      "enum": ["clear", "attention", "not_processed"],
      "description": "Overall outcome of the proof of address check"
    },
    "extracted_data": {
      "type": "object",
      "description": "Data extracted from the document",
      "properties": {
        "address": {
          "type": "object",
          "properties": {
            "line1": { "type": "string" },
            "line2": { "type": "string" },
            "city": { "type": "string" },
            "state": { "type": "string" },
            "postalCode": { "type": "string" },
            "country": { "type": "string" }
          }
        },
        "document_type": { "type": "string" },
        "document_date": { "type": "string" }
      }
    },
    "client_validation": {
      "type": "object",
      "description": "Validation results against client information"
    },
    "content_analysis": {
      "type": "object",
      "description": "Analysis of document content and authenticity"
    },
    "geolocation_analysis": {
      "type": "object",
      "description": "Geolocation verification of the address"
    },
    "document_breakdown": {
      "type": "object",
      "description": "Detailed breakdown of document analysis"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp when the check was created"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp when the check was last updated"
    }
  },
  "required": ["check_id", "outcome"]
}